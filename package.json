{
  "name": "portfolio",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "next lint",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "precommit": "lint-staged",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build:pages": "next build && next export -o docs"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^11.2.6",
    "lucide-react": "^0.441.0",
    "next": "14.2.4",
    "next-themes": "^0.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.2.6",
    "@storybook/addon-interactions": "^8.2.6",
    "@storybook/addon-links": "^8.2.6",
    "@storybook/nextjs": "^8.2.6",
    "@storybook/react": "^8.2.6",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "critters": "^0.0.25",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.4",
    "husky": "^9.1.1",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.3",
    "vitest": "^2.0.5"
  },
  "lint-staged": {
    "*{js,jsx,ts,tsx,md,css,scss,json}": [
      "prettier --write",
      "eslint --fix --max-warnings=0"
    ]
  }
}
